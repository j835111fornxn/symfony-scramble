{
    "name": "dedoc/scramble",
    "description": "Automatic generation of API documentation for Symfony applications.",
    "keywords": [
        "symfony",
        "documentation",
        "openapi",
        "api",
        "swagger"
    ],
    "homepage": "https://github.com/dedoc/scramble",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Lytvynenko",
            "email": "litvinenko95@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/config": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/event-dispatcher": "^6.4|^7.0",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/routing": "^6.4|^7.0",
        "symfony/serializer": "^6.4|^7.0",
        "symfony/twig-bundle": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/orm": "^2.17|^3.0",
        "twig/twig": "^3.8",
        "myclabs/deep-copy": "^1.12",
        "nikic/php-parser": "^5.0",
        "phpstan/phpdoc-parser": "^1.0|^2.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^6.4|^7.0",
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "symfony/var-dumper": "^6.4|^7.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^1.10|^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.2|^2.0",
        "phpstan/phpstan-phpunit": "^1.4|^2.0",
        "phpstan/phpstan-symfony": "^1.3|^2.0",
        "phpunit/phpunit": "^10.5|^11.5.3",
        "pestphp/pest": "^2.34|^3.7",
        "spatie/pest-plugin-snapshots": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.48"
    },
    "autoload": {
        "psr-4": {
            "Dedoc\\Scramble\\": "src",
            "Dedoc\\Scramble\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dedoc\\Scramble\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "symfony": {
            "require": "^6.4|^7.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}