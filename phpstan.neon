includes:
    - phpstan-baseline.neon

parameters:
    level: 9
    paths:
        - src
        - config
    stubFiles:
        - stubs/DeepCopy.stub
    tmpDir: build/phpstan
    typeAliases:
        Rule: 'string|\Symfony\Component\Validator\Constraint'
        RuleSet: 'Rule|Rule[]'
    reportUnmatchedIgnoredErrors: false
